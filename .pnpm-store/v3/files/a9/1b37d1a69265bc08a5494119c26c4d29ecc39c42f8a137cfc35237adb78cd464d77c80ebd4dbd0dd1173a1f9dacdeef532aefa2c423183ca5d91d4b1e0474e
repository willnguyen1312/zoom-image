    const originalRange = {
        start: { line: start.line, character: start.character },
        end: { line: end.line, character: end.character }
    };
    checkRangeLength(originalRange, range);
    return {
        range: originalRange,
        uri: start.uri ? start.uri : fragment.getURL()
    };
}
