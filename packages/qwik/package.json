{
  "name": "@zoom-image/qwik",
  "version": "0.2.10",
  "description": "Adapter of zoom image core for Qwik",
  "author": "Nam Nguyen <nam.nguyen.node@gmail.com>",
  "license": "MIT",
  "repository": "willnguyen1312/zoom-image",
  "homepage": "https://zoom-images.netlify.app",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/willnguyen1312"
  },
  "keywords": [
    "javascript",
    "typescript",
    "zoom",
    "image",
    "hover",
    "move",
    "wheel",
    "pinch",
    "touch",
    "scroll",
    "mobile",
    "desktop",
    "qwik"
  ],
  "main": "./lib/index.qwik.mjs",
  "qwik": "./lib/index.qwik.mjs",
  "types": "./lib-types/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.qwik.mjs",
      "require": "./lib/index.qwik.cjs",
      "types": "./lib-types/index.d.ts"
    }
  },
  "files": [
    "lib",
    "lib-types",
    "src"
  ],
  "scripts": {
    "build": "qwik build",
    "build.lib": "vite build --mode lib",
    "build.types": "tsc --emitDeclarationOnly",
    "dev.lib": "vite build --mode lib --watch",
    "dev.types": "tsc --emitDeclarationOnly --watch",
    "dev": "run-p dev.lib dev.types",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"src/**/*.ts*\"",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "1.4.5",
    "eslint-config-custom": "workspace:*",
    "npm-run-all": "^4.1.5",
    "prettier": "3.2.5",
    "typescript": "5.3.3",
    "undici": "6.6.2",
    "vite": "^4.5.2"
  },
  "dependencies": {
    "@namnode/utils": "^0.4.0",
    "@zoom-image/core": "workspace:*"
  }
}
