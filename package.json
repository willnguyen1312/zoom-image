{
  "name": "zoom-image",
  "private": true,
  "author": "Nam Nguyen <nam.nguyen.node@gmail.com>",
  "license": "MIT",
  "repository": "willnguyen1312/zoom-image",
  "homepage": "https://zoom-images.netlify.app",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/willnguyen1312"
  },
  "keywords": [
    "javascript",
    "typescript",
    "zoom",
    "image",
    "hover",
    "move",
    "wheel",
    "pinch",
    "touch",
    "scroll",
    "mobile",
    "desktop"
  ],
  "scripts": {
    "test": "turbo run test",
    "build": "turbo run build",
    "postbuild": "pnpm run size",
    "size": "esno scripts/size-limit.ts",
    "core": "pnpm --filter=@zoom-image/core",
    "react": "pnpm --filter=@zoom-image/react",
    "docs": "pnpm --filter=docs",
    "build:docs": "turbo run build --filter=docs... --force",
    "start-docs": "turbo run dev --filter=docs...",
    "start-preact": "turbo run dev --filter=preact-ts...",
    "start-react": "turbo run dev --filter=@zoom-image/react --filter=react-ts...",
    "start-svelte": "turbo run dev --filter=svelte-ts...",
    "start-vanilla": "turbo run dev --filter=vanilla-ts...",
    "start-vue": "turbo run dev --filter=vue-ts...",
    "start-solid": "turbo run dev --filter=solid-ts...",
    "start-qwik": "turbo run dev --filter=qwik-ts...",
    "typecheck": "turbo run typecheck",
    "format:check": "prettier --check ./**/*.{js,ts,tsx,mjs,json,vue,svelte,md}",
    "format:fix": "prettier --write ./**/*.{js,ts,tsx,mjs,json,vue,svelte,md}",
    "lint": "turbo run lint",
    "changeset": "changeset",
    "release": "pnpm check && changeset version && changeset publish",
    "check": "turbo run build typecheck lint test --filter=!docs",
    "turbo:update": "pnpx @turbo/codemod update"
  },
  "dependencies": {
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.1",
    "@zoom-image/core": "workspace:*",
    "eslint": "^8.43.0",
    "eslint-config-custom": "workspace:*",
    "esno": "^0.16.3",
    "export-size": "^0.5.2",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "turbo": "^1.10.6",
    "unocss": "^0.53.3"
  }
}
