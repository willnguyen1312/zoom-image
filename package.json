{
  "name": "zoom-image",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/willnguyen1312/zoom-image"
  },
  "keywords": [
    "javascript",
    "typescript",
    "zoom",
    "image",
    "hover",
    "move",
    "wheel",
    "pinch",
    "touch",
    "scroll",
    "mobile",
    "desktop"
  ],
  "author": "Nam Nguyen <nam.nguyen.node@gmail.com>",
  "license": "MIT",
  "scripts": {
    "packages": "pnpm --filter=./packages/** -r",
    "examples": "pnpm --filter=./examples/** -r",
    "build-fast": "pnpm packages -- build-fast",
    "dev": "pnpm packages -- dev",
    "test": "pnpm packages -- test",
    "test:ci": "pnpm packages -- test:ci",
    "build": "pnpm packages -- build && node scripts/build.mjs",
    "size": "pnpm build && size-limit",
    "deploy:docs": "pnpm docs:build && netlify deploy --prod --dir=./docs/.vitepress/dist",
    "preact": "pnpm --filter \"./examples/preact-ts\"",
    "react": "pnpm --filter \"./examples/react-ts\"",
    "svelte": "pnpm --filter \"./examples/svelte-ts\"",
    "vanilla": "pnpm --filter \"./examples/vanilla-ts\"",
    "vue": "pnpm --filter \"./examples/vue-ts\"",
    "solid": "pnpm --filter \"./examples/solid-ts\"",
    "qwik": "pnpm --filter \"./examples/qwik-ts\"",
    "core": "pnpm --filter \"./packages/core\"",
    "start:preact": "pnpm preact dev --open --host",
    "start-preact": "run-p dev start:preact",
    "start:react": "pnpm react dev --open --host",
    "start-react": "run-p dev start:react",
    "start:svelte": "pnpm svelte dev --open --host",
    "start-svelte": "run-p dev start:svelte",
    "start:vanilla": "pnpm vanilla dev --open --host",
    "start-vanilla": "run-p dev start:vanilla",
    "start:vue": "pnpm vue dev --open --host",
    "start-vue": "run-p dev start:vue",
    "start:solid": "pnpm solid dev --open --host",
    "start-solid": "run-p dev start:solid",
    "start:qwik": "pnpm qwik dev --open --host",
    "start-qwik": "run-p dev start:qwik",
    "typecheck": "pnpm packages -- typecheck",
    "prettier": "prettier --check **/*",
    "prettier-fix": "prettier --write packages/** examples/**",
    "lint": "eslint packages --ext .ts",
    "docs:dev": "vitepress dev docs --open --host",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "changeset": "changeset",
    "release": "changeset publish"
  },
  "dependencies": {
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.1",
    "@size-limit/preset-small-lib": "^8.2.4",
    "@zoom-image/core": "workspace:*",
    "eslint": "8.40.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "size-limit": "^8.2.4",
    "ts-morph": "^18.0.0",
    "tsup": "6.7.0",
    "tsx": "3.12.7",
    "typescript": "5.0.4",
    "unocss": "^0.51.13",
    "vitepress": "1.0.0-alpha.75",
    "vue": "^3.3.2"
  },
  "size-limit": [
    {
      "name": "createZoomImageWheel",
      "path": "packages/core/dist/index.mjs",
      "import": "{ createZoomImageWheel }",
      "limit": "1.5KB"
    },
    {
      "name": "createZoomImageHover",
      "path": "packages/core/dist/index.mjs",
      "import": "{ createZoomImageHover }",
      "limit": "1.5KB"
    },
    {
      "name": "createZoomImageMove",
      "path": "packages/core/dist/index.mjs",
      "import": "{ createZoomImageMove }",
      "limit": "1.5KB"
    },
    {
      "name": "createZoomImageClick",
      "path": "packages/core/dist/index.mjs",
      "import": "{ createZoomImageClick }",
      "limit": "1.5KB"
    },
    {
      "name": "cropImage",
      "path": "packages/core/dist/index.mjs",
      "import": "{ cropImage }",
      "limit": "1.5KB"
    },
    {
      "name": "makeCalculatePercentage",
      "path": "packages/core/dist/index.mjs",
      "import": "{ makeCalculatePercentage }",
      "limit": "1.5KB"
    },
    {
      "name": "makeCalculateZoom",
      "path": "packages/core/dist/index.mjs",
      "import": "{ makeCalculateZoom }",
      "limit": "1.5KB"
    }
  ]
}
