{
  "name": "zoom-image",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/willnguyen1312/zoom-image"
  },
  "author": "Nam Nguyen <nam.nguyen.node@gmail.com>",
  "license": "MIT",
  "scripts": {
    "packages": "pnpm --filter=./packages/** -r",
    "examples": "pnpm --filter=./examples/** -r",
    "build-fast": "pnpm packages -- build-fast",
    "build": "pnpm packages -- build",
    "deploy:demo": "netlify deploy --prod --dir=./examples/vanilla-ts/dist",
    "preact": "pnpm --filter \"./examples/react-ts\"",
    "react": "pnpm --filter \"./examples/react-ts\"",
    "svelte": "pnpm --filter \"./examples/svelte-ts\"",
    "vanilla": "pnpm --filter \"./examples/vanilla-ts\"",
    "vue": "pnpm --filter \"./examples/vue-ts\"",
    "start-preact": "pnpm preact dev",
    "start-react": "pnpm react dev",
    "start-svelte": "pnpm svelte dev",
    "start-vanilla": "pnpm vanilla dev",
    "start-vue": "pnpm vue dev",
    "typecheck": "pnpm packages -- typecheck",
    "prettier": "prettier --check **/*",
    "prettier-fix": "prettier --write packages/** examples/**",
    "lint": "eslint packages --ext .ts",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "changeset": "changeset",
    "release": "changeset publish"
  },
  "dependencies": {
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.1",
    "eslint": "8.39.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "prettier": "2.8.8",
    "ts-morph": "^18.0.0",
    "tsup": "6.7.0",
    "tsx": "3.12.7",
    "typescript": "5.0.4",
    "unocss": "^0.51.8",
    "vitepress": "1.0.0-alpha.75",
    "vue": "^3.2.47"
  }
}
