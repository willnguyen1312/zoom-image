  "scripts": {
    "packages": "pnpm --filter=./packages/** -r",
    "build-fast": "pnpm packages -- build-fast",
    "build": "pnpm packages -- build",
    "preact": "pnpm --filter \"./examples/react-ts\"",
    "react": "pnpm --filter \"./examples/react-ts\"",
    "svelte": "pnpm --filter \"./examples/svelte-ts\"",
    "vanilla": "pnpm --filter \"./examples/vanilla-ts\"",
    "vue": "pnpm --filter \"./examples/vue-ts\"",
    "start-preact": "pnpm preact dev",
    "start-react": "pnpm react dev",
    "start-svelte": "pnpm svelte dev",
    "start-vanilla": "pnpm vanilla dev",
    "start-vue": "pnpm vue dev",
    "typecheck": "pnpm packages -- typecheck",
    "prettier": "prettier --check **/*",
    "prettier-fix": "prettier --write **/*",
    "lint": "eslint packages --ext .ts"
  },
  "dependencies": {
    "eslint": "8.39.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
